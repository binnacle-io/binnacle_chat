<%= javascript_tag(id: 'binnacle_remove') do %>
$("body").data("binnacle",
  {
    accountId: '<%= binnacle_chat_account %>',
    appId: '<%= binnacle_chat_app %>',
    apiKey: '<%= binnacle_chat_api_key %>',
    apiSecret: '<%= binnacle_chat_api_secret %>',
    endPoint: '<%= binnacle_chat_url %>',
    contextId: '<%= binnacle_chat_context %>',
    identity: '<%= binnacle_chat_identity %>',
    room: '<%= binnacle_chat_room %>'
  }
);
<% end %>

<div class="binnacle_chat" id="<%= binnacle_chat_id %>">
  <div id="messages">
    <div class="chat-item item-left clearfix">
      <div class="chat-user">
        <img src="http://placehold.it/50x50" class="img-responsive img-circle">
      </div>
      <div class="chat-body">
        <div class="name">Batman</div>
        <div class="time hidden-xs">May 21, 2015 18:28</div>
        <div class="text">Using my detective skills, I've determined your true identity.</div>
      </div>
    </div> <!-- /chat-item -->
    <div class="chat-item item-right clearfix">
      <div class="chat-user">
        <img src="http://placehold.it/50x50" class="img-responsive img-circle">
      </div>
      <div class="chat-body">
        <div class="name">Superman</div>
        <div class="time hidden-xs">May 21, 2015 18:30</div>
        <div class="text">This is awkward...</div>
      </div>
    </div> <!-- /chat-item -->
    <div class="chat-item item-right clearfix">
      <div class="chat-user">
        <img src="http://placehold.it/50x50" class="img-responsive img-circle">
      </div>
      <div class="chat-body">
        <div class="name">Superman</div>
        <div class="time hidden-xs">May 21, 2015 18:33</div>
        <div class="text">...</div>
      </div>
    </div> <!-- /chat-item -->
  </div>

  <legend id="username"></legend>
  
  

  <form class="form" id="chat-form">
    <input id="message" type="text" class="form-control" placeholder="Type somethingâ€¦" />
  </form>
</div>